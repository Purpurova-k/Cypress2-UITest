    "cy:spec2:cloud": "cypress run --spec 'cypress/e2e/book-tickets/book-tickets.cy.js' --record --key 545de53a-f15f-4e9c-b50f-b2b02bbd6e0e",
    "cy:parallel:cloud": "npm run cy:spec1:cloud -b chrome --group admin --ci-build-id 1 & npm run cy:spec2:cloud -b chrome --group book-tickets --ci-build-id 1"
  },
